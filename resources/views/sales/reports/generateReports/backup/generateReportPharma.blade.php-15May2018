			   @if(!empty($viewData['orderTrackRecord']['finalizing']['user_signature']) && file_exists(DOC_ROOT.SIGN_PATH.$viewData['orderTrackRecord']['finalizing']['user_signature']))
			      <span style="bottom:-55px !important;right:0;position:relative;width:100%;">
			      <img style="width:0px;height:80px;margin-bottom:-15px!important;float: left;" alt="{{$viewData['orderTrackRecord']['finalizing']['user_signature']}}" src="{{DOC_ROOT.SIGN_PATH.$viewData['orderTrackRecord']['finalizing']['user_signature']}}"/></span>
			   @endif
			</p>
			<p style="font-size:13px;padding-top:-5px!important;margin-right:30px !important;"><b>{{!empty($viewData['orderTrackRecord']['finalizing']['username']) ? $viewData['orderTrackRecord']['finalizing']['username'] : '-'}}</b></p>
			<p style="font-size:13px;padding-top:-5px!important;"><b>[Person Incharge / DGM Pharma]</b></p>
		     </td>
		  </tr>
	      </table>
	</div>
